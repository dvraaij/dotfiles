# Bash
.bashrc         .config/bash/rc
.bash_profile   .config/bash/profile
.bash_logout    .config/bash/logout
.bash_aliases   .config/bash/aliases
.env            .config/bash/environment

# X11
.Xresources     .config/X11/resources

# Editors
.emacs.d        .config/emacs/
.vimrc          .config/vim/rc

# Other
.ssh            .config/ssh/
.gitconfig      .config/git/config
.gtkrc-2.0      .config/gtk-2.0/settings.ini
.icons          .config/icons/

# Other (not-tracked)
.mozilla        .config/mozilla/
